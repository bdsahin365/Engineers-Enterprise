{
    "kind": "collectionType",
    "collectionName": "products",
    "info": {
        "singularName": "product",
        "pluralName": "products",
        "displayName": "Product",
        "description": "Construction material products"
    },
    "options": {
        "draftAndPublish": true
    },
    "pluginOptions": {},
    "attributes": {
        "name": {
            "type": "string",
            "required": true
        },
        "modelNo": {
            "type": "string"
        },
        "category": {
            "type": "enumeration",
            "enum": [
                "PORCH_PILLAR",
                "CLEAR_COVERING",
                "FANCY_BLOCK",
                "WINDOW_PILLAR",
                "BALCONY_PILLAR",
                "BALUSTER",
                "ROOF_CORNICE",
                "OTHER"
            ]
        },
        "description": {
            "type": "text"
        },
        "images": {
            "type": "media",
            "multiple": true,
            "required": false,
            "allowedTypes": [
                "images"
            ]
        },
        "isPillar": {
            "type": "boolean",
            "default": false
        },
        "isVisible": {
            "type": "boolean",
            "default": true
        },
        "price": {
            "type": "decimal"
        },
        "pillarConfig": {
            "type": "json"
        }
    }
}